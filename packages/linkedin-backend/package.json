{
  "name": "linkedin-backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-old": "rimraf ./dist && tsx ./scripts/build.ts",
    "dev": "rimraf ./dist && tsup src/main.ts --format esm --watch --onSuccess 'node dist/main.js'",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.esm.mjs",
    "blah": "tsx src/main.ts ",
    "blah2": "ts-node-dev src/main.ts ",
    "start:dev": "npx ts-node-dev --respawn --clear --rs --transpile-only --tree-kill --exit-child --require tsconfig-paths/register src/main.ts | npx pino-pretty --translateTime \"UTC:yyyy-mm-dd'T'HH:MM:ss'Z'\""
  },
  "devDependencies": {
    "@graphql-tools/schema": "^9.0.19",
    "@graphql-tools/utils": "^9.2.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.10",
    "@types/pg": "^8.6.6",
    "@types/ws": "^8.5.4",
    "drizzle-kit": "^0.17.6",
    "esbuild": "^0.17.14",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "@apollo/server": "^4.7.0",
    "@as-integrations/fastify": "^1.3.2",
    "@fastify/compress": "^6.2.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^10.1.0",
    "@fastify/rate-limit": "^8.0.0",
    "@godaddy/terminus": "^4.12.0",
    "apollo-server": "^3.12.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "colors.ts": "^1.0.20",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.25.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "fastify": "^4.17.0",
    "fastify-healthcheck": "^4.4.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.21.3",
    "internal-ip": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "type-graphql": "2.0.0-beta.1",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
